pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Build') {
            steps {
                echo 'Welcome to Building'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Welcome to Deploying'
            }
        }
        stage('Test') {
            steps {
                echo 'Welcome to Testing'
            }
        }
        stage('Release') {
            steps {
                echo 'Welcome to Release '
            }
        }
    }
}
